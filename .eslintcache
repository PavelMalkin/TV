any_address}</Typography>\r\n            <Typography> {contact.phone}</Typography>\r\n            <Typography> {'lat:' + props.contact.coordinates.lat + ' lng:' + props.contact.coordinates.lng}</Typography>\r\n        </Grid>\r\n    );\r\n\r\n    return (\r\n        <Grid item >\r\n            <Paper>\r\n                <Grid container\r\n                      wrap=\"wrap\"\r\n                      direction=\"row\"\r\n                      justify=\"space-around\"\r\n                      alignItems=\"center\"\r\n                      alignContent=\"center\"\r\n                >\r\n\r\n                    <Grid item xs={4}>\r\n                        <Grid container\r\n                              justify='center'\r\n                              alignItems=\"flex-start\"\r\n                              direction=\"column\">\r\n                            <Grid item xs={11}>\r\n                                <CardActionArea>\r\n                                    <CardMedia\r\n                                        component=\"img\"\r\n                                        height=\"140\"\r\n                                        image={contact.img}\r\n                                    />\r\n                                    <Typography gutterBottom >\r\n                                        {contact.position}\r\n                                    </Typography>\r\n                                </CardActionArea>\r\n\r\n                            </Grid>\r\n\r\n                        </Grid>\r\n                    </Grid>\r\n\r\n                    {description}\r\n\r\n                </Grid>\r\n                <Grid container\r\n                      justify='flex-end'>\r\n                    <Grid item>\r\n                        <IconButton onClick={handleClickChangeButton}>\r\n                            <EditIcon/>\r\n                        </IconButton>\r\n                        <IconButton onClick={handleDelete}>\r\n                         